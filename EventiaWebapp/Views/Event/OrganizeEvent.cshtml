@using EventiaWebapp.Services
@using Microsoft.AspNetCore.Identity
@using EventiaWebapp.Models
@inject OrganizerHandler _organizer
@inject UserManager<User> _userManager
@model EventiaWebapp.Models.Event




@{
    var organizer = await _userManager.GetUserAsync(User);
    var organizerModel = await _organizer.SingleOrganizerEventList(organizer);
}


<table class="table">
    <thead>
    <tr>
        <th scope="col">Title</th>
        <th scope="col">Date</th>
        <th scope="col">Description</th>
        <th scope="col">Place</th>
    </tr>
    </thead>


    <tbody>
    
    @foreach (var item in organizerModel)
    {
        <tr>
            <td>@item.Title</td>
            <td>@item.Date</td>
            <td>@item.Description</td>
            <td>@item.Place</td>
        </tr>
    } 

    </tbody> 
</table> 